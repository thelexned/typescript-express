{
  "name": "typescript-express",
  "version": "1.0.0",
  "description": "Example project for using typescript with express.",
  "main": "dist/main.js",
  "scripts": {
    "compile": "npx tsc",
    "start": "npm run compile && node ./dist/main.js",
    "start-dev": "node ./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run start\"",
    "lint": "npx tslint --project tsconfig.json --config tslint.json",
    "test": "jest",
    "test-with-coverage": "jest --coverage"
  },
  "keywords": ["typescript", "express", "jest", "tslint"],
  "author": "Lex Ned",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.9",
    "@types/body-parser": "^1.19.0",
    "@types/multer": "^1.4.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.4",
    "typedi": "^0.8.0"
  }
}
